{
  "manifest_version": 3,
  "name": "Open Source YouTube Shorts Blocker",
  "version": "1.0",
  "permissions": ["scripting", "tabs"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["osysb.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "./osysb.html"
  }
}
